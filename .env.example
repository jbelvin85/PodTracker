# .env.example
# The Architect: Central configuration blueprint.
# Copy this file to .env and fill in the values.

# Docker Compose variables
COMPOSE_PROJECT_NAME=podtracker

# PostgreSQL Database
POSTGRES_USER=admin
POSTGRES_PASSWORD=password
POSTGRES_DB=podtracker
DATABASE_URL="postgresql://admin:password@db:5432/podtracker?schema=public"

# Backend JWT Secret
JWT_SECRET=your-super-secret-key
